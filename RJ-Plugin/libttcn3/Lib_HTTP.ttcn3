module Lib_Http {
	type record HTTPResponse {
		StatusLine statusLine, set of Header headers, Body body optional
	}
	with {
		encode "HTTP/response"
	}

	type record StatusLine {
		integer statusCode, charstring reasonPhrase optional
	}

	type record Body {
		charstring messageBodyTxt optional, octetstring messageBodyRaw optional
	}

	type record Header {
		charstring name, charstring val
	}
}
